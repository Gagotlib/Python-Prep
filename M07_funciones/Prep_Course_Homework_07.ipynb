    "def factorial(numero):\n",
    "    if type(numero) != int:\n",
    "        return \"no ingresaste un numero entero\"\n",
    "    if numero < 0:\n",
    "        return \"no ingresaste numero positivo\"\n",
    "    if numero ==1:\n",
    "        return 1\n",
    "   \n",
    "    numero = numero * factorial(numero-1)\n",
    "    return numero \n",
    "factorial(5)"
   ]
