    "def esprimo (n):\n",
    "    primo = True\n",
    "    for i in range (2,n):\n",
    "        if n % i == 0:\n",
    "            primo = False\n",
    "            return primo\n",
    "            break\n",
    "        else:\n",
    "            return primo\n",
    "\n",
    "esprimo(7)"
   ]
